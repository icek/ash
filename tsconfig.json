{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "isolatedModules": true,
    "lib": ["dom", "es6"],
    "baseUrl": "./packages",
    "paths": {
      "@ash.ts/*": ["./*/src"]
    }
  },
  "typedocOptions": {
    "name": "@ash.ts",
    "entryPointStrategy": "packages",
    "entryPoints": ["."],
    "out": "docs",
    "excludeExternals": true,
    "excludePrivate": true,
    "excludeProtected": false,
    "excludeInternal": true,
    "gitRevision": "master",
    "includes": "./markdown/typedoc",
    "readme": "./markdown/typedoc/index.md"
  }
}
